{
  "name": "@airclass/toybox",
  "version": "0.3.13",
  "description": "React components library extend antd",
  "author": "DivXPro",
  "license": "MIT",
  "repository": "DivXPro/toybox",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --css-modules false --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --css-modules false --format modern,cjs",
    "release": "npm publish --access public",
    "release:next": "npm publish --access public --tag next",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "deploy": "gh-pages -d example/build"
  },
  "devDependencies": {
    "@mdx-js/mdx": "^1.6.16",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^25.1.4",
    "@types/node": "^12.12.38",
    "@types/qs": "^6.9.5",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.7",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-window": "^1.8.2",
    "@types/react-window-infinite-loader": "^1.0.3",
    "@types/styled-components": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.intersection": "^4.4.6",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gatsby-plugin-sass": "^2.3.13",
    "gh-pages": "^2.2.0",
    "less": "^3.12.2",
    "microbundle-crl": "^0.13.10",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "react-styleguidist": "^11.1.0",
    "typescript": "^3.7.5"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@airclass/icons": "^4.3.4",
    "ahooks": "^2.6.1",
    "antd": "^4.9.1",
    "classnames": "^2.2.6",
    "dayjs": "^1.8.36",
    "lodash.debounce": "^4.0.8",
    "lodash.intersection": "^4.4.0",
    "immutability-helper": "^3.1.1",
    "moment": "^2.29.0",
    "qs": "^6.9.4",
    "styled-components": "^5.2.0"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "ahooks": "^2.6.1",
    "antd": "^4.9.1",
    "moment": "^2.29.0",
    "styled-components": "^5.2.0"
  }
}
